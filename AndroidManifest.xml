<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- Permission untuk akses internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/apkstar"
        android:label="@string/app_name"
        android:roundIcon="@drawable/apkstar"
        android:supportsRtl="true"
        android:theme="@style/Theme.ApkStar"
        tools:targetApi="31">

        <!-- Main Activity, entry point aplikasi -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Activity-activity lain -->
        <activity android:name=".ActivityB" />
        <activity android:name=".ActivityC" />
        <activity android:name=".ActivityD" />
        <activity android:name=".ActivityE" />
        <activity android:name=".ActivityF" />

    </application>

</manifest>
